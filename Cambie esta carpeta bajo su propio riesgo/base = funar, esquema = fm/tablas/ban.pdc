-- Create table
create table BAN
(
  id_ban  NUMBER(10) not null,
  usuario VARCHAR2(21)
)
tablespace FM_DATA
  pctfree 10
  initrans 1
  maxtrans 255;
-- Create/Recreate primary, unique and foreign key constraints 
alter table BAN
  add constraint PK_BAN primary key (ID_BAN)
  using index 
  tablespace FM_DATA
  pctfree 10
  initrans 2
  maxtrans 255;
alter table BAN
  add constraint FK_USUA foreign key (USUARIO)
  references USUARIO (USUARIO);
